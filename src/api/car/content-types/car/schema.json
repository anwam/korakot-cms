{
  "kind": "collectionType",
  "collectionName": "cars",
  "info": {
    "singularName": "car",
    "pluralName": "cars",
    "displayName": "Car",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "make": {
      "type": "string"
    },
    "year": {
      "type": "integer"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "variant": {
      "type": "string"
    },
    "engine_capacity": {
      "type": "decimal"
    },
    "plate_number": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "slug": {
      "type": "uid",
      "targetField": "plate_number"
    },
    "car_type": {
      "type": "enumeration",
      "enum": [
        "sedan",
        "pickup truck",
        "suv",
        "hatchback"
      ]
    },
    "models": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "down_payment": {
      "type": "decimal"
    },
    "installment_period": {
      "type": "integer"
    },
    "transmission": {
      "type": "enumeration",
      "enum": [
        "automatic",
        "manual"
      ]
    },
    "fuel_type": {
      "type": "enumeration",
      "enum": [
        "gasoline",
        "diesel",
        "electric"
      ]
    }
  }
}
